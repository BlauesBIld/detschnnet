    </header>
    <!-- header-area-end -->

    <!-- hero-area-start -->
    <section class="hero container">
        <div class="hero__flex">
            <div class="hero__left">
                <div class="game-title">
                    <h1><%= game.title %></h1>
                </div>
                <div class="game-description">
                    <%- game.descriptionHtml %>
                </div>
            </div>
            <div class="hero__right mt-5">
                <div class="aspect-ratio-wrapper">
                    <div class="owl-carousel owl-theme">
                        <% imageNames.forEach(function(imageName){ %>
                            <div class="item"><img src="/img/games/<%= game.uuid %>/<%= imageName %>" alt="Game Image"></div>
                        <% }); %>
                    </div>
                </div>
                <div class="justify-content-center mt-3">
                    <div class="boxed-data row justify-content-center">
                        <div class="col-9">
                            <div class="border rounded-3 border-secondary p-3">
                                <div class="row">
                                    <div class="col-md-6">
                                        <p>Engine:</p>
                                        <p>Language(s)/Tools:</p>
                                        <p>Status:</p>
                                        <p>Genre:</p>
                                        <p>Platform:</p>
                                        <p>Github:</p>
                                        <p>Tags:</p>
                                    </div>
                                    <div class="col-md-6">
                                        <p><b><%= game.engine %></b></p>
                                        <p><b><%= game.languages_and_tools %></b></p>
                                        <p><b><%= game.status %></b></p>
                                        <p><b><%= game.genre %></b></p>
                                        <p><b><%= game.platform %></b></p>
                                        <p>
                                            <% if (game.github && game.github.trim() !== '') { %>
                                                <a href="<%= game.github %>" target="_blank">Repository</a>
                                            <% } else { %>
                                                <s>Repository</s>
                                            <% } %>
                                        </p>
                                        <p><b><%= game.tags %></b></p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- hero-area-end -->
</div>


<!-- jQuery first, then Popper.js, then Bootstrap JS -->
<script src="/js/jquery.min.js"></script>
<script src="/js/Popper.js"></script>
<script src="/js/owl.carousel.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/main.js"></script>

<script>
    $(document).ready(function () {
        $(".owl-carousel").owlCarousel({
            items: 1,
            nav: true,
            dot: false,
            loop: true,
            margin: 0,
            autoplay: false,
            autoplayTimeout: 6000,
            smartSpeed: 1000,
            responsiveClass: true,
            responsive: {
                0: {
                    items: 1,
                },
                768: {
                    items: 1,
                },
                1000: {
                    items: 1,
                }
            }
        });
    });
</script>

</body>

